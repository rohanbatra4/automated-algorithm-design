    <localRunParameters>
        <workersPerHost>1</workersPerHost>
    </localRunParameters>
    <slurmEngineParameters>
        <numHosts>3</numHosts>
        <workersPerHost>1</workersPerHost>
	      <jobName>emade.titanic</jobName>
	      <runtime></runtime> <!--Optional runtime in D-HH:MM -->
